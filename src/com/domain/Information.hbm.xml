<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.domain.Information" table="information">
    <id name="iid" column="iid">
      <generator class="native"/>
    </id>
    <property name="iname" column="iname" length="20"/>
    <property name="iactor" column="iactor" length="20"/>
    <property name="idirect" column="idirect" length="20"/>
    <property name="itime" column="itime" />
   
    <property name="idescription" column="idescription" length="50"/>
    
  </class>
</hibernate-mapping>